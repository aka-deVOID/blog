// #111111, #555555, #777777, #999999
export global Palette {
    in-out property <color> background: #212121;
    in-out property <color> middle-black: #323232;
    in-out property <color> middle-blue: #0d7377 ;
    in-out property <color> blue: #14ffec;
    in-out property <color> text: #ffffff ;
}